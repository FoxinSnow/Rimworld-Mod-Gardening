<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="FurnitureBase">
        <defName>Large_Fountain</defName>
        <label>large fountain</label>
        <description>A large fountain to bring you happiness.</description>
        <designationCategory>Gardening</designationCategory>
        
        <selectable>true</selectable>
        <fillPercent>0.90</fillPercent>
            
        <graphicData>
            <texPath>Large_Fountain_tex</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(5,5)</drawSize>
            <shadowData>
                <volume>(0.3, 0.6, 0.3)</volume>
                <offset>(0.0, 0.0, -0.1)</offset>
            </shadowData>
        </graphicData>
        
        <statBases>
            <MaxHitPoints>900</MaxHitPoints>
            <Flammability>0</Flammability>
            <Beauty>100</Beauty>
            <WorkToBuild>1500</WorkToBuild>
            <Mass>200</Mass> <!--weight-->
        </statBases>
        <socialPropernessMatters>true</socialPropernessMatters>
        <size>(5,5)</size>
        <stuffCategories>
            <li>Woody</li>
            <li>Stony</li>
        </stuffCategories>
        <costStuffCount>300</costStuffCount>
        <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
        <techLevel>Medieval</techLevel>
    </ThingDef>
	
</Defs>
